{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_WHACK_A_MONKEY_ADDRESS": "@vite_whack_a_monkey_address",
    "VITE_SIGNER_PRIVATE_KEY": "@vite_signer_private_key"
  }
} 